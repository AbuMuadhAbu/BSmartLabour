    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Smart Labour </title>
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0" name="viewport"  id="viewport">

    @Scripts.Render("~/bundles/jquery")
    <link href="~/Content/Front/css/reset.css" type="text/css" rel="stylesheet" />
    <link href="~/Content/Front/css/style.css" type="text/css" rel="stylesheet" />

    @Styles.Render("~/Content/Front/css")
