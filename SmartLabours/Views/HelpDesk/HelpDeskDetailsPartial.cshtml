    <script type="text/javascript">

        var SearchText = '@ViewData["SearchText"]';

        function ReOpenIssue() {

        }

        function CloseIssue() {

        }


    </script>


    <div class="LastCollapse">
        <div class="content">

            <div style="text-align:center">
                <table border="0" cellspacing="0" cellpadding="3" class="TitleTable">

                    <tr>
                        <td class="right">
                            TokenID :
                        </td>
                        <td class="left">@ViewBag.TokenID </td>

                        <td width="200" class="right">
                            Issue Type :
                        </td>

                        <td class="left">@ViewBag.IssueType </td>
                        <td width="100" class="right">
                            Priority :
                        </td>
                        <td class="left">@ViewBag.IssuePriority </td>
                    </tr>
                    <tr>
                        <td class="right">
                            Created Date :
                        </td>
                        <td class="left">@ViewBag.CreateDate </td>

                <td class="right">
                    Last Updated Date :
                </td>
                <td class="left">@ViewBag.Date </td>
            </tr>

        </table>

    </div>

    <table border="0" cellspacing="0" cellpadding="3">
        <thead>
            <tr>

                <td class="sno1">
                    S.No.
                </td>

                <td class="date1">
                    Date
                </td>

                <td class="description1">
                    Description
                </td>

                <td class="status1">
                    Status
                </td>

                <td class="Responsible1">
                    Responsible
                </td>


            </tr>
        </thead>

        <tbody>
            @{

                int sno = 0;
            }

            @foreach (var item in Model)
            {
                sno = sno + 1;
                <tr>
                    <td class="sno1">
                        @(sno)
                    </td>

                    <td class="date1">
                        @Html.DisplayFor(modelItem => item.StrEDC)
                    </td>

                    <td class="status1">
                        @Html.DisplayFor(modelItem => item.Description)
                    </td>

                    <td class="status1">
                        @Html.DisplayFor(modelItem => item.IssueStatus)
                    </td>

                    <td class="Responsible1">
                        @Html.DisplayFor(modelItem => item.Responsible)
                    </td>
                </tr>
            }
        </tbody>
    </table>

</div>

</div>
