@model SmartLabours.Models.TBL_SERVICEPROVIDER_SMT
@{
    ViewBag.Title = "Service Provider View";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<!DOCTYPE html>
<html>
<head>
    <title>Banner View</title>
    <script type="text/javascript">
        function PreviousPage() {

            window.location = "/Service/ServiceProvider_List";
        }

    </script>
</head>
<body>
    @* Google Analytics COde  *@
    <noscript><iframe src="//www.googletagmanager.com/ns.html?id=GTM-PTWMKL" height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <script>(function (w, d, s, l, i) { w[l] = w[l] || []; w[l].push({ 'gtm.start': new Date().getTime(), event: 'gtm.js' }); var f = d.getElementsByTagName(s)[0], j = d.createElement(s), dl = l != 'dataLayer' ? '&l=' + l : ''; j.async = true; j.src = '//www.googletagmanager.com/gtm.js?id=' + i + dl; f.parentNode.insertBefore(j, f); })(window, document, 'script', 'dataLayer', 'GTM-PTWMKL');</script>
    <!-- End here -->
    <div class="content">
        <div class="wrapper">
            <div class="continn">
                <fieldset>
                    <legend></legend>
                    <h3>
                        Service Provider View
                    </h3>
                    <div class="clr2">
                    </div>
                    <div class="fld_fd view-blk">
                        
                        @if (HttpContext.Current.Session["USERID"].ToString() == "1")
                        {

                             <dl>
                            <dd>
                                Organization</dd> 
                            <dt>
                                @Html.DisplayFor(n => n.Organizations.OrganisationName)</dt>
                        </dl>
                            
                        }
                        <dl>
                            <dd>
                                Service Provider
                            </dd>
                            <dt>
                                @Html.DisplayFor(n => n.SERVICEPROVIDER)
                            </dt>
                        </dl>

                        <dl>
                            <dd>
                                Is Telcom Operator
                            </dd>
                            <dt>
                                @{if (Model.IsTelcomOperator == true)
                                {<label>Yes</label>}
                                else
                                {<label>No</label>}}
                            </dt>
                        </dl>



                        <dl>
                            <dd>
                                Status</dd>
                            <dt>
                                @{if (Model.STATUS == true)
                                  {<label>Active</label>}
                                  else
                                  {<label>In Active</label>}}
                            </dt>
                        </dl>
                        <div class="btn1 btn1-form">
                            <input type="button" value="Back" title="Back" onclick="PreviousPage();" />
                        </div>
                    </div>
                </fieldset>
            </div>
        </div>
    </div>
</body>
</html>
