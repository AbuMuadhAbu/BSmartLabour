    <ul class="testimonial_list clearfix" id="js_testimonial2">
        @{
                                  int a = 1;
                                  foreach (var items in Model)
                                  {
                                    <li>
                                        <div class="testimg_div">

                                            @if (items.LabourImage != null && items.LabourImage != "")
                                              {
                                                <img src="~/Content/Front/images/LabourImage/@items.LabourImage" width="238" height="188" alt="" />
                                              }
                                              else
                                              {
                                                <img src="~/Content/Front/images/LabourImage/team_no-img.png" width="238" height="188" alt="" />

                                              }
                                        </div>

                                        <h3>
                                            @Html.DisplayFor(modelitem => items.Name)
                                        </h3>

                                    </li>
                                              a = a + 1;
                                  }
        }

    </ul>

