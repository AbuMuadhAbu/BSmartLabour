<div class="content">
    <div class="wrapper">
        <div class="continn">
            <h3>
                Sponsor View
            </h3>
            <div class="clr2">
            </div>
            <div class="fld_fd view-blk">
                <dl>
                    <dd>
                        Emirates ID :</dd>
                    <dt>
                        @Html.DisplayFor(model => model.EmiratesId)
                    </dt>
                </dl>
                <dl>
                    <dd>
                        Name :</dd>
                    <dt>
                        @Html.DisplayFor(model => model.Name)
                    </dt>
                </dl>
                <dl>
                    <dd>
                        Email ID
                    </dd>
                    <dt>
                        @Html.DisplayFor(model => model.EmailID)
                    </dt>
                </dl>
                <dl>
                    <dd>
                        Phone No:
                    </dd>
                    <dt>
                        @Html.DisplayFor(model => model.PhoneNumber)
                    </dt>
                </dl>

           
                @if (HttpContext.Current.Session["USERID"].ToString() == "1")
                {

                    <dl>
                        <dd>
                            Organization
                        </dd>
                        <dt>
                            @Html.DisplayFor(n => n.Organizations.OrganisationName)
                        </dt>
                    </dl>

                }




                <dl>
                    <dd>
                        Address:
                    </dd>
                    <dt>
                        @Html.DisplayFor(model => model.Address1)
                        @*@Html.DisplayFor(model => model.Address2)*@
                    </dt>
                </dl>
                 <dl>
                    <dd>
                        Pincode:
                    </dd>
                    <dt>
                        @Html.DisplayFor(model => model.Pincode)
                    
                    </dt>
                </dl>
                @*<dl>
                    <dd>
                        service Provider:
                    </dd>
                    <dt>
                        @Html.DisplayFor(model => model..SERVICEPROVIDER)
                    </dt>
                </dl>*@


                <dl>
                    <dd>
                        Created Date:
                    </dd>
                    <dt>
                        @Html.DisplayFor(model => model.CREATEDDATE)
                    </dt>
                </dl>

                <dl>
                    <dd>
                        Status :</dd>
                    <dt>
                        @{if (Model.Status == true)
                          {<label>Active</label>}
                          else
                          {<label>In Active</label>}}
                    </dt>
                </dl>
            </div>
            <div class="btn1">
                <a href="/SmartUsers/Sponsor_List" title="Back" class="green">Back</a>
            </div>
        </div>
    </div>
</div>