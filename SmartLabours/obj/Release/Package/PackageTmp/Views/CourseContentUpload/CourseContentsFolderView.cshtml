<div class="content">
    <div class="wrapper">
        <div class="continn">
            <fieldset>
                <legend></legend>
                <h3>
                    Course Content Directory View
                </h3>
                <div class="clr2">
                </div>
                <div class="fld_fd view-blk">
                  

                    @if (HttpContext.Current.Session["USERID"].ToString() == "1")
                    {

                        <dl>
                            <dd>
                                Organization
                            </dd>
                            <dt>
                                @Html.DisplayFor(n => n.Organizations.OrganisationName)
                            </dt>
                        </dl>

                    }

                    <dl>
                        <dd>
                            Directory
                        </dd>
                        <dt>
                            @Html.DisplayFor(n => n.FolderName)
                        </dt>

                    </dl>

                    <dl>
                        <dd>
                            File Type
                        </dd>
                        <dt>
                            @{if (Model.FileTypes == true)
                            {<label>Audio</label>}
                            else
                            {<label>Image</label>}}
                        </dt>
                    </dl>

                    <div class="btn1 btn1-form">
                        <input type="button" value="Back" title="Back" onclick="PreviousPage();" />
                    </div>
                </div>
            </fieldset>
        </div>
    </div>
</div>

